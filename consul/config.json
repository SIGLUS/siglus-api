{
  "service": {
    "Name": "siglusapi",
    "Port": 8080,
    "Tags": ["openlmis-service"],
    "check": {
      "interval": "10s",
      "http": "http://HOST:PORT/health"
    }
  },
  "raml": "src/main/resources/api-definition.yaml",
  "path": [
    "siglusapi",
    "siglusapi/docs",
    "siglusapi/docs/<all>",
    "siglusapi/webjars",
    "siglusapi/webjars/<all>"
  ]
}
