{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "RequisitionPeriodDto",
  "description": "A processing period combined with id and status of a related requisition",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "processingSchedule": {
      "type": "object",
      "$ref": "processingScheduleDto.json",
      "title": "processingSchedule"
    },
    "name": {
      "type": "string",
      "title": "name"
    },
    "description": {
      "type": "string",
      "title": "description"
    },
    "startDate": {
      "type": "string",
      "title": "startDate"
    },
    "endDate": {
      "type": "string",
      "title": "endDate"
    },
    "durationInMonths": {
      "type": "integer",
      "title": "durationInMonths"
    },
    "requisitionId": {
      "type": "string",
      "title": "requisitionId"
    },
    "requisitionStatus": {
      "type": "string",
      "title": "requisitionStatus"
    }
  },
  "required": [
    "id",
    "processingSchedule",
    "name",
    "description",
    "startDate",
    "endDate"
  ]
}
