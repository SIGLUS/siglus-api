{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "The response for a single product for android device",
  "properties": {
    "productCode": {
      "type": "string"
    },
    "archived": {
      "type": "boolean"
    },
    "fullProductName": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "netContent": {
      "type": "integer"
    },
    "packRoundingThreshold": {
      "type": "integer"
    },
    "roundToZero": {
      "type": "boolean"
    },
    "programCode": {
      "type": "string"
    },
    "children": {
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "productChild.json"
      }
    },
    "isBasic": {
      "type": "boolean"
    },
    "isKit": {
      "type": "boolean"
    },
    "isNos": {
      "type": "boolean"
    },
    "isHiv": {
      "type": "boolean"
    },
    "lastUpdated": {
      "type": "string"
    }
  }
}
