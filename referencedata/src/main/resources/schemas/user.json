{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "User",
  "description": "A single user",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "username": {
      "type": "string",
      "title": "username"
    },
    "firstName": {
      "type": "string",
      "title": "firstName"
    },
    "lastName": {
      "type": "string",
      "title": "lastName"
    },
    "jobTitle": {
      "type": [
        "string",
        "null"
      ],
      "title": "jobTitle"
    },
    "timezone": {
      "type": "string",
      "title": "timezone"
    },
    "homeFacility": {
      "type": "object",
      "$ref": "facility.json",
      "title": "homeFacility"
    },
    "active": {
      "type": "boolean",
      "title": "active"
    },
    "roleAssignments": {
      "type": "array",
      "title": "roleAssignments",
      "items": {
        "type": "object",
        "$ref": "roleAssignmentDto.json"
      },
      "uniqueItems": false
    },
    "extraData": {
      "type": "object",
      "title": "extraData"
    }
  },
  "required": [
    "username",
    "firstName",
    "lastName",
    "active"
  ]
}
