{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "title": "RequisitionGroupObjectReference",
  "description": "A single requisition group",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "code": {
      "type": "string",
      "unique": true,
      "title": "code"
    },
    "name": {
      "type": "string",
      "title": "name"
    },
    "description": {
      "type": "string",
      "title": "description"
    },
    "supervisoryNode": {
      "type": "object",
      "$ref": "supervisoryNodeDto.json"
    },
    "requisitionGroupProgramSchedules": {
      "type": "array",
      "title": "requisitionGroupProgramSchedules",
      "items": { "type": "object", "$ref": "requisitionGroupProgramScheduleDto.json" },
      "uniqueItems": false
    },
    "memberFacilities": {
      "type": "array",
      "title": "memberFacilities",
      "items": { "type": "object", "$ref": "facility.json" },
      "uniqueItems": false
    },
    "href": {
      "type": "string",
      "title": "href"
    }
  },
  "required": ["id"]
}