{   "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "ProofOfDeliveryLineItemDto",
  "description": "A single proofOfDeliveryLineItem ",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "orderable": {
      "type": ["object"],
      "$ref": "versionObjectReference.json",
      "title": "orderable"
    },
    "lot": {
      "type": ["object", "null"],
      "$ref": "referenceObjectDto.json",
      "title": "lot"
    },
    "quantityAccepted": {
      "type": "integer",
      "title": "quantityAccepted"
    },
    "useVvm": {
      "type": "boolean",
      "title": "useVvm"
    },
    "vvmStatus": {
      "type": "string",
      "title": "vvmStatus"
    },
    "quantityRejected": {
      "type": "integer",
      "title": "quantityRejected"
    },
    "rejectionReasonId": {
      "type": "string",
      "title": "rejectionReasonId"
    },
    "notes": {
      "type": "string",
      "title": "notes"
    }
  },
  "required": [
    "orderable"
  ]
}
