{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "ShipmentDraft",
  "description": "A shipment draft",
  "properties": {
    "id": {
      "type": ["string", "null"],
      "title": "id"
    },
    "order": {
      "type": ["object"],
      "$ref": "referenceObjectDto.json",
      "title": "order"
    },
    "notes": {
      "type": ["string", "null"],
      "title": "notes"
    },
    "lineItems": {
      "type": ["array"],
      "title": "line items",
      "items": {
        "type": "object",
        "$ref": "shipmentLineItem.json"
      },
      "uniqueItems": true
    }
  },
  "required": [
    "lineItems",
    "order"
  ]
}
