{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "The response for a single facilityinfo for android device",
  "title": "entity",
  "properties": {
    "code": {
      "type": "integer",
      "title": "code"
    },
    "name": {
      "type": "string",
      "title": "name"
    },
    "supportedPrograms": {
      "type": "array",
      "title": "supportedPrograms",
      "items": {
        "type": "object",
        "$ref": "program.json"
      }
    },
    "supportedReportTypes": {
      "type": "array",
      "title": "supportedReportTypes",
      "items": {
        "type": "object",
        "$ref": "reportType.json"
      }
    },
    "isAndroid": {
      "type": "boolean",
      "title": "isAndroid"
    }
  }
}
